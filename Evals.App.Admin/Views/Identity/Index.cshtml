@{
    Layout = null;
}

<div class="bg-light lter b-b wrapper-md app-header-fixed">
    <form role="form">
        <div class="form-group">
            <label>Email address</label>
            <input type="email" class="form-control" placeholder="Enter email">
        </div>
        <div class="form-group">
            <label>Password</label>
            <input type="password" class="form-control" placeholder="Password">
        </div>
        <div class="checkbox">
            <label class="i-checks">
                <input type="checkbox" checked disabled><i></i> Check me out
            </label>
        </div>
        <button type="submit" class="btn btn-sm btn-primary">Submit</button>
    </form>

</div>
<div class="wrapper-md">
    <div class="row">
        @for (int i = 0; i < 20; i++)
        {
            <div class="col-lg-2 col-md-3 col-xs-4">
                <div class="panel panel-info">
                    <div class="panel-body">
                        <a href="" class="thumb pull-left avatar">
                            <img src="img/a4.jpg" alt="...">
                            <i class="on md b-white bottom"></i>
                        </a>
                        <div class="clear m-l-xs">
                            <a href="" class="text-info block m-b-xs">Mike Mcalidek Mcalidek<i class="icon-twitter"></i></a>
                            <a href="" class="text-danger block m-t-xs"><i class="fa fa-eye"></i> Follow</a>
                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>